seznam = [12, 13, 14, 5, 6, 9, 8]

def sum_of(seznam):
    vsota = sum(seznam)
    print vsota

def minimun(seznam):
    minimun = min(seznam)
    print minimun

def max_min (seznam):
    maximum = max(seznam)
    minimum = min(seznam)
    print maximum, minimum

def max_minus_min(seznam):
    maximum = max(seznam)
    minimum = min(seznam)
    print maximum - minimum

def average(seznam):
    average = sum(seznam)/len(seznam)
    print average

sum_of(seznam)
minimun(seznam)
max_min(seznam)
max_minus_min(seznam)
average(seznam)
